<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            Title="Belleza App"
            xmlns:local="clr-namespace:BellezaTestApp.Views"
             x:Class="BellezaTestApp.Views.MenuPage">
    <FlyoutPage.Flyout>
        <local:FlyoutMenuPage x:Name="flyout"/>

    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:HomePage/>
            </x:Arguments>
        </NavigationPage>
        
    </FlyoutPage.Detail>
    
</FlyoutPage>